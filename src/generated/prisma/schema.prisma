generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Song {
  id     Int    @id @default(autoincrement())
  title  String @db.VarChar(100)
  artist String @db.VarChar(100)
  year   Int
}

model Save {
  id     Int    @id @default(autoincrement())
  songId Int
  title  String @db.VarChar(100)
}

